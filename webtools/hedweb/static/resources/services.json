{
    "services": {
        "get_services": {
            "Name": "get_services",
            "Description": "Return a list of hed services commands with descriptions",
            "Parameters": [],
            "Returns": [
                "supported_services"
            ]
        },
        "events_assemble": {
            "Name": "events_assemble",
            "Description": "Validate a BIDS events file with JSON sidecars (passed as strings)",
            "Parameters": [
                "events_file_string",
                [
                    "hed_version",
                    "hed_schema_string"
                ],
                "json_dictionary_strings"
            ],
            "Returns": [
                "command",
                "data",
                "hed_version",
                "msg",
                "status_category"
            ]
        },
        "events_validate": {
            "Name": "events_validate",
            "Description": "Validate a BIDS events file with JSON sidecars (passed as strings)",
            "Parameters": [
                "events_file_string",
                [
                    "hed_version",
                    "hed_schema_string"
                ],
                "json_dictionary_strings"
            ],
            "Returns": [
                "command",
                "data",
                "hed_version",
                "msg",
                "status_category"
            ]
        },
        "json_to_long": {
            "Description": "JSON sidecar as a string",
            "Parameters": [
                "events_file_string",
                [
                    "hed_version",
                    "hed_schema_string"
                ],
                "json_dictionary_string"
            ],
            "Returns": [
                "command",
                "data",
                "hed_version",
                "msg",
                "status_category"
            ]
        },
        "json_to_short": {
            "Description": "Convert a BIDS JSON sidecar (in string form) to short form if valid, otherwise return errors",
            "Parameters": [
                "service",
                [
                    "hed_version",
                    "hed_schema_string"
                ],
                "json_dictionary_string"
            ],
            "Returns": [
                "command",
                "data",
                "hed_version",
                "msg",
                "status_category"
            ]
        },
        "json_validate": {
            "Description": "Validate a BIDS JSON sidecar (in string form) and return errors",
            "Parameters": [
                "service",
                [
                    "hed_version",
                    "hed_schema_string"
                ],
                "json_dictionary_string"
            ],
            "Returns": [
                "command",
                "data",
                "hed_version",
                "msg",
                "status_category"
            ]
        },
        "spreadsheet_validate": {
            "Description": "Validate a spreadsheet (tsv string) with HED tags in specified columns",
            "Parameters": [
                "service",
                [
                    "hed_version",
                    "hed_schema_string"
                ],
                "spreadsheet_file_string"
            ],
            "Returns": [
                "command",
                "data",
                "hed_version",
                "msg",
                "status_category"
            ]
        },
        "strings_validate": {
            "Description": "Validate a list of HED strings and return errors",
            "Parameters": [
                "service",
                "hed_strings",
                [
                    "hed_version",
                    "hed_schema_string"
                ]
            ],
            "Returns": [
                "command",
                "data",
                "hed_version",
                "msg",
                "status_category"
            ]
        }
    },
    "meanings": {
        "command": "Operation to perform.",
        "data": "Data returned by the service.",
        "events_file_string": "A BIDS events file as a string",
        "hed_columns": "A list of column numbers (starting with 1) of columns containing HED strings. If empty, all columns are used.",
        "hed_strings": "List of HED strings to be processed",
        "hed_version": "Version of HED to used in processing",
        "json_dictionary_string": "JSON sidecar as a string",
        "json_dictionary_strings": "A list of BIDS JSON sidecars as strings",
        "spreadsheet_file_string": "A spreadsheet tsv as a string",
        "supported_services": "A list of available services with description, parameters and return values.",
        "status_category": "Type of return value"
    }
}